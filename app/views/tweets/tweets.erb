<h2>Welcome, <%= current_user.username %></h2>

<h3>All Tweets</h3>
<ul>
<% @tweets.each do |tweet| %>
    <li>
      <a href="/tweets/<%= tweet.id %>"><%= tweet.content %></a>
    </li>
  <% end %>
</ul>

<a href="/tweets/new"><button>New Tweet</button></a>
<a href="/logout"><button>Log Out</button></a>